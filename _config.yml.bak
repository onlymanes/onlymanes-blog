# _config.yml
title: onlymanes Blog
url: "https://onlymanes.ai"
baseurl: ""

theme: minima

plugins:
  - jekyll-feed
  - jekyll-seo-tag

defaults:
  - scope:
      path: ""
    values:
      head: |
        <!-- 加 relative_url，任何基路径都能正确引用 -->
        <link rel="stylesheet" href="{{ '/assets/css/custom.css' | relative_url }}">

include:
  - _includes/footer.html